      ENABLEDIMAGE = NULL;
    END

    WINDOW
      509 219 76 200 16 STATICTEXT;
      STATUS = DISABLED+SMOOTH_TEXT+NOFOCUS;
      DATA = 1 0 WindowDir:Blank;
    END
  END
