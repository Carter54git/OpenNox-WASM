      STYLE = MOUSETRACK;
      IMAGEOFFSET = -399 -368;
      HILITEIMAGE = SelCharDeleteButtonLit;
      SELECTEDIMAGE = SelCharDeleteButtonLit;
      BACKGROUNDIMAGE = NULL;
      ENABLEDIMAGE = NULL;
      DISABLEDIMAGE = SelCharDeleteButtonDisabled;
      TEXT = SelChar.wnd:Delete;
    END

    
    WINDOW
      510 108 106 400 233 SCROLLLISTBOX (Character List);
      STATUS = ENABLED+NOINPUT+NOFOCUS;
      ENABLEDCOLOR = 0 0 0;
      HILITECOLOR = 0 0 0;
      DISABLEDCOLOR = 128 128 128;
      SELECTEDCOLOR = 255 128 128;
      BACKGROUNDCOLOR = 0 0 0;
      TEXTCOLOR = 240 180 42;
      DATA = 128 17 0 0 0 0 1;

      CHILD
        WINDOW
          511 0 0 130 233 SCROLLLISTBOX (Character Names);
          STATUS = ENABLED+ONE_LINE+NOFOCUS;
          SELECTEDCOLOR = 128 128 128;
          HILITECOLOR = TRANSPARENT;
          DISABLEDCOLOR = 0 0 0;
          BACKGROUNDCOLOR = TRANSPARENT;
          TEXTCOLOR = 240 180 42;
          DATA = 128 17 0 0 0 0 1;
        END

        WINDOW
          512 130 0 270 233 SCROLLLISTBOX (Character Style);
          STATUS = ENABLED+ONE_LINE+NOFOCUS;
          SELECTEDCOLOR = 128 128 128;
          HILITECOLOR = TRANSPARENT;
          DISABLEDCOLOR = 0 0 0;
          BACKGROUNDCOLOR = TRANSPARENT;
          TEXTCOLOR = 240 180 42;
          DATA = 128 17 0 0 0 0 1;
        END
      END
    END

    WINDOW
      504 512 106 20 25 PUSHBUTTON (UP button);
      STATUS = ENABLED+NOFOCUS+IMAGE;
      STYLE = MOUSETRACK;
      BACKGROUNDIMAGE = ShopInventoryUp;
      SELECTEDIMAGE = ShopInventoryUpSelected;
      HILITEIMAGE = ShopInventoryUpSelected;
