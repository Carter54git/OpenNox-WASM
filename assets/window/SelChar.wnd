SELECTEDCOLOR = TRANSPARENT;
TEXTCOLOR = 240 180 42;
BACKGROUNDCOLOR = TRANSPARENT;
FONT = large;


WINDOW
  500 0 0 640 480 USER;
  STATUS = ENABLED+IMAGE+NOFOCUS;
  BACKGROUNDIMAGE = SelCharBG;

  CHILD
    WINDOW
      501 125 368 116 22 PUSHBUTTON;
      STATUS = IMAGE+SMOOTH_TEXT+NOFOCUS;
      STYLE = MOUSETRACK;
      IMAGEOFFSET = -125 -368;
      HILITEIMAGE = SelCharNewButtonLit;
      SELECTEDIMAGE = SelCharNewButtonLit;
      BACKGROUNDIMAGE = NULL;
      ENABLEDIMAGE = NULL;
      DISABLEDIMAGE = SelCharNewButtonDisabled;
      TEXT = SelChar.wnd:New;
    END

    WINDOW
      502 262 368 116 22 PUSHBUTTON;
      STATUS = ENABLED+IMAGE+SMOOTH_TEXT+NOFOCUS;
