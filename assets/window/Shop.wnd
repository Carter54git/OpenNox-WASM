  CHILD

    WINDOW
      3806 10 10 300 100 USER (vendor inventory area);
      STATUS = ENABLED+NOFOCUS;
    END

    WINDOW
      3805 521 4 100 100 USER (vendor pic);
      STATUS = IMAGE+ENABLED+NOFOCUS;
      BACKGROUNDIMAGE = ShopKeeperPic;
      SELECTEDIMAGE  = NULL;
      DISABLEDIMAGE  = NULL;
      HILITEIMAGE = NULL;
      ENABLEDIMAGE   = NULL;
    END
    
    WINDOW
      3804 345 4 92 23 PUSHBUTTON (buy button);
      STATUS = ENABLED+NOFOCUS;
      STYLE = MOUSETRACK;
      TEXT = Shop.wnd:Buy;
      ENABLEDCOLOR = TRANSPARENT;
      HILITECOLOR = TRANSPARENT;
      SELECTEDCOLOR = TRANSPARENT;
      BACKGROUNDCOLOR = TRANSPARENT;
    END

    WINDOW
      3802 345 35 92 23 PUSHBUTTON (sell button);
      STATUS = ENABLED+NOFOCUS;
      STYLE = MOUSETRACK;
      TEXT = Shop.wnd:Sell;
      ENABLEDCOLOR = TRANSPARENT;
      HILITECOLOR = TRANSPARENT;
      SELECTEDCOLOR = TRANSPARENT;
      BACKGROUNDCOLOR = TRANSPARENT;
    END

    WINDOW
      3803 345 65 92 23 PUSHBUTTON (Repair button);
      STATUS = ENABLED+NOFOCUS;
      STYLE = MOUSETRACK;
