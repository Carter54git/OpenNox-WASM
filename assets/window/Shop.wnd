      ENABLEDCOLOR = TRANSPARENT;
      HILITECOLOR = TRANSPARENT;
      SELECTEDCOLOR = TRANSPARENT;
      BACKGROUNDCOLOR = TRANSPARENT;
    END

    WINDOW
      3807 313 42 16 30 VERTSLIDER (vendor inventory slider);
      STATUS = ENABLED+NOFOCUS;
      STYLE = MOUSETRACK;
      ENABLEDCOLOR = TRANSPARENT;
      HILITECOLOR = TRANSPARENT;
      SELECTEDCOLOR = TRANSPARENT;
      BACKGROUNDCOLOR = TRANSPARENT;
      DATA = 0 300;
    END

    WINDOW
      3808 311 5 20 25 PUSHBUTTON (inventory up button);
      STATUS = ENABLED+NOFOCUS+IMAGE;
      STYLE = MOUSETRACK;
      BACKGROUNDIMAGE = ShopInventoryUp;
