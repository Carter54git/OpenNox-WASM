      HILITEIMAGE = NULL;
      ENABLEDIMAGE = NULL;
    END

    WINDOW
      3809 311 91 20 25 PUSHBUTTON (inventory down button);
      STATUS = ENABLED+NOFOCUS+IMAGE;
