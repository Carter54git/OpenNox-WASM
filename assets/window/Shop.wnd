      BACKGROUNDIMAGE = ShopInventoryDown;
      SELECTEDIMAGE = ShopInventoryDownSelected;
      DISABLEDIMAGE = NULL;
      HILITEIMAGE = NULL;
      ENABLEDIMAGE = NULL;
    END

    WINDOW
      3810 535 -25 100 20 STATICTEXT (talker name);
      STATUS = NOFOCUS;
      DISABLEDCOLOR = 0 0 0;
      HILITECOLOR = 0 0 0;
      TEXTCOLOR = 230 165 65;
      DATA = 0 0 Dialog.wnd:Empty;
    END

